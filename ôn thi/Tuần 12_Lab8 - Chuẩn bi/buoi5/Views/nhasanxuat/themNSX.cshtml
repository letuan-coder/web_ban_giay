@model buoi5.Models.Nhasanxuat

@{
    ViewData["Title"] = "themNSX";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h4>Thêm nhà sản xuất</h4>
<hr />
<p>
    <a asp-action="Index" class="btn btn-primary">Quay lại </a>
</p>
<div class="row">
    <div class="col-md-12">
        <form asp-action="themNSX">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Mansx" class="control-label"></label>
                <input asp-for="Mansx" class="form-control" />
                <span asp-validation-for="Mansx" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Tennsx" class="control-label"></label>
                <input asp-for="Tennsx" class="form-control" />
                <span asp-validation-for="Tennsx" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Diachi" class="control-label"></label>
                <input asp-for="Diachi" class="form-control" />
                <span asp-validation-for="Diachi" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Thêm" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


